Algoritmo "somador"

Var
   cont, n, s: inteiro


Inicio
        cont <- 1
        s <-0
        Enquanto (cont <= 5) faca
                 Escreva("Digite um numero para somar: ")
                 Leia(n)
                 s <- s + n
                 cont <- cont + 1

        FimEnquanto
        Escreva("Seus números somados são ", s)
Fimalgoritmo